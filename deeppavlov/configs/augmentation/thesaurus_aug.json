{
  "chainer": {
    "in": [
      "x"
    ],
    "pipe": [
      {
        "in": "x",
        "out": "x_tok",
        "id": "my_tokenizer",
        "class_name": "ru_tokenizer"
      },
      {
        "in": [
          "x_tok"
        ],
        "out": [
          "aug_x_tok"
        ],
        "class_name": "thesaurus_aug",
        "lang": "rus",
        "penalty_for_source_token": 0.01,
        "kenlm_model_path": "{MODELS_PATH}/lms/ru_wiyalen_no_punkt.arpa.binary",
        "ru_thes_dirpath": "{DOWNLOADS_PATH}/ruthes_lite2/",
        "replace_freq": 1.0,
        "with_source_token": true,
        "force_inflect": false
      },
      {
        "in": "aug_x_tok",
        "out": "aug_x",
        "class_name": "ru_tokenizer"
      }
    ],
    "out": [
      "aug_x"
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "/home/azat/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models"
    }
  }
}
